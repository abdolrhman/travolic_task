{
  "author": "Abdulrahman Soliman <iwcw4i@gmail.com>",
  "dependencies": {
    "axios": "^0.19.0",
    "bcryptjs": "2.4.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.0",
    "compression": "^1.6.2",
    "cors": "^2.8.3",
    "cross-env": "^6.0.3",
    "dotenv-safe": "^6.0.0",
    "email-templates": "^6.0.3",
    "express": "^4.15.2",
    "express-validation": "^1.0.2",
    "helmet": "^3.5.0",
    "http-status": "^1.0.1",
    "joi": "^10.4.1",
    "jwt-simple": "0.5.6",
    "lodash": "^4.17.15",
    "method-override": "^3.0.0",
    "moment-timezone": "^0.5.13",
    "morgan": "^1.8.1",
    "nodemailer": "^6.3.1",
    "pm2": "^4.2.0",
    "pug": "^2.0.4",
    "uuid": "^3.1.0",
    "winston": "^3.1.0"
  },
  "description": "consume API, produce others with more func",
  "devDependencies": {
    "apidoc": "^0.17.5",
    "coveralls": "^3.0.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-base": "^12.0.1",
    "eslint-plugin-import": "^2.2.0",
    "husky": "^3.0.7",
    "jest": "^25.2.7",
    "nodemon": "^2.0.1",
    "nyc": "^14.1.1",
    "opn-cli": "^5.0.0",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.0.0",
    "supertest": "^4.0.2"
  },
  "engines": {
    "node": ">=8",
    "yarn": "*"
  },
  "license": "MIT",
  "main": "src/index.js",
  "name": "travolic_task",
  "private": false,
  "scripts": {
    "precommit": "yarn lint",
    "prestart": "yarn docs",
    "start": "cross-env NODE_ENV=production pm2 start ./src/index.js",
    "dev": "nodemon ./src/index.js",
    "lint": "eslint ./src/ --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:fix": "yarn lint --fix",
    "lint:watch": "yarn lint --watch",
    "test": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text jest --testTimeout=30000 --detectOpenHandles src/api/tests",
    "test:unit": "cross-env NODE_ENV=test jest src/api/tests/unit",
    "test:integration": "cross-env NODE_ENV=test jest --testTimeout=10000 src/api/tests/integration",
    "test:watch": "cross-env NODE_ENV=test jest --testTimeout=10000 --watch src/api/tests/unit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "validate": "yarn lint && yarn test",
    "postpublish": "git push --tags",
    "docs": "apidoc -i src -o docs",
    "postdocs": "opn docs/index.html",
    "docker:start": "cross-env NODE_ENV=production pm2-docker start ./src/index.js"
  },
  "version": "1.2.2"
}
